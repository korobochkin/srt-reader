{
    "name": "korobochkin/srt-reader",
    "description": "Reader of srt files",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Korobochkin\\SrtReader\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Korobochkin\\SrtReader\\Tests\\Integration\\": "tests/integration"
        }
    },
    "authors": [
        {
            "name": "Nicolas Korobochkin",
            "homepage": "https://github.com/korobochkin/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.4",
        "phplrt/runtime": "^3.7"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92",
        "phplrt/phplrt": "^3.7",
        "vimeo/psalm": "^6.14",
        "phpunit/phpunit": "^12.5",
        "psalm/plugin-phpunit": "^0.19.5"
    }
}
